	<!-- config: singletons -->

	<object name="life-cycle" type="System.Collections.Generic.List&lt;string&gt;">
		<constructor-arg index="0">
			<list element-type="string">
				<value>bootstrap</value>
				<value>parse-request</value>
				<!--<value>authenticate</value>-->
